#TB_overlay {
    background: rgba(0,0,0,.5);
}
#TB_window {
    background: #fff;
    margin: 0 !important;
    transform: translate(-50%, -50%);
    width: 1200px!important;
    height: 600px;
    max-width: 90vw;
    max-height: 90vh;
}

#TB_window iframe {
    width: 100% !important;
    height: 100% !important;
}

#TB_closeWindowButton {
    background-image: url("data:image/svg+xml;base64,");
    background-size: 100% 100%;
    display: block;
    width: 22px;
    height: 22px;
    font-size:0;
}