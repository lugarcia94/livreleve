$star 
    background-image: url('data:image/svg+xml;base64,')
    background-repeat: no-repeat
    background-size: 200px auto
    background-position: -100px -2px
    content: ''     
    display: block
    height: 15px
    width: 90px

star01 = -104px -2px
star02 = -85px -2px
star03 = -65px -2px
star04 = -46px -2px
star05 = -8px -2px


.review

    &--short
        display: flex
        align-items: center

        strong 
            display: none

    .rating-produto
        order: 2
        color: color[0]
        margin: 0 0 0 5px
        font-weight: 600
        font-size: 13px

        &:before 
            content: '('

        &:after 
            content: ')'
            
    &__star
        order: 1
        &:before 
            @extend $star 


    .avaliacao0
    .avaliacao10
        + .review__star
            &:before 
                background-position: star01

    .avaliacao15
    .avaliacao20
        + .review__star
            &:before 
                background-position: star02

    .avaliacao25
    .avaliacao30
        + .review__star
            &:before 
                background-position: star03

    .avaliacao35
    .avaliacao40
        + .review__star
            &:before 
                background-position: star04

    .avaliacao45
    .avaliacao50
        + .review__star
            &:before 
                background-position: star05
            
            

   



            


