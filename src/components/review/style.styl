.avaliacao

    .rating-wrapper
        display: flex
        flex-wrap: wrap
        strong 
            display: none
        .rating
            display: block;
            height: 15px;
            width: 90px;
            margin: 0 0 0 10px
            &:before 
                @extend $star 

    .a0
    .a10
    .avaliacao0
    .avaliacao10
        &:before 
            background-position: star01 !important

    .a15
    .a20
    .avaliacao15
    .avaliacao20
        &:before 
            background-position: star02 !important

    .a25
    .a30
