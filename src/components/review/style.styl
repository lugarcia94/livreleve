$star 
    background-image: url('data:image/svg+xml;base64,')
    background-repeat: no-repeat
    background-size: 200px auto
    background-position: -100px -2px
    content: ''     
    display: block
    height: 15px
    width: 90px

star01 = -104px -2px
star02 = -85px -2px
star03 = -65px -2px
star04 = -46px -2px
star05 = -8px -2px


.review

    &--short
        display: flex
        align-items: center


        strong 
            display: none

    .rating-produto
        order: 2
        color: color[0]
        margin: 0 0 0 5px
        font-weight: 600
        font-size: 13px

        &:before 
            content: '('

        &:after 
            content: ')'
            
    &__star
        order: 1
        &:before 
            @extend $star 


    .avaliacao0
    .avaliacao10
        + .review__star
            &:before 
                background-position: star01

    .avaliacao15
    .avaliacao20
        + .review__star
            &:before 
                background-position: star02

    .avaliacao25
    .avaliacao30
        + .review__star
            &:before 
                background-position: star03

    .avaliacao35
    .avaliacao40
        + .review__star
            &:before 
                background-position: star04

    .avaliacao45
    .avaliacao50
        + .review__star
            &:before 
                background-position: star05
            

#opiniao_de_usuario
    hr 
        display: none

    .rating-demonstrativo
    .rating-produto
        &:before 
            @extend $star 

    .avaliacao0
    .avaliacao10
        &:before 
            background-position: star01

    .avaliacao15
    .avaliacao20
        &:before 
            background-position: star02

    .avaliacao25
    .avaliacao30
        &:before 
            background-position: star03

    .avaliacao35
    .avaliacao40
        &:before 
            background-position: star04

    .avaliacao45
    .avaliacao50
        &:before 
            background-position: star05
   

    .user-review 
        h4 
            display: none

        .avaliacao
            display: grid
            @media(min-width: 769px)
                grid-template-areas: "media avalie-produto" "rating avalie-produto" "rating ulPubliqueResenha" "rating  links-resenha-usuario" "resenhas resenhas" "userReviewReturnMessages userReviewReturnMessages"
                grid-template-columns: 390px auto

            @media(max-width: 768px)
                grid-template-areas: "media media" "rating rating" "avalie-produto avalie-produto" "ulPubliqueResenha ulPubliqueResenha" "links-resenha-usuario links-resenha-usuario" "resenhas resenhas"  "userReviewReturnMessages userReviewReturnMessages"
                

        .media
            grid-area: media
            display: flex
            width: 100%
            margin: 0 0 7px 0
            font-size: 13px
            color: #333333
            letter-spacing: -0.24px
            font-weight: 600
            text-transform: capitalize
            @media(min-width: 769px)
                max-width: 390px

            span 
                order: 2

            em 
                display: block 
                width: 140px
                order: 1
                strong 
                    &:after 
                        display: none

                span 
                    display: none

        .rating
            grid-area: rating
            width: 100%
            
            + p 
                display: none

            li 
                display: flex
                justify-content: space-between
                @media(max-width: 768px)
                    justify-content: initial

            .voteRatingBar
                display: none
                + span 
                    width: 64%
                    font-size: 13px
                    color: #333333
                    letter-spacing: -0.24px
                    font-weight: 600
                    text-transform: capitalize
                    @media(max-width: 768px)
                        width: auto

            .voteRatingStar
                display: block
                height: 15px
                width: 140px

        .avalie-produto
            grid-area: avalie-produto
            display: flex
            flex-wrap: wrap
            @media(max-width: 768px)
                margin: 15px 0 0 0

            #opcoes-avalie
                margin: 5px 0
                width: 100%
                p  
                    display: none

                input 
                    display: inline-block
                label 
                    display: inline-block
                    margin: 0 10px 0 0
                    font-size: 13px
                    color: #333333
                    letter-spacing: -0.24px
                    font-weight: 600
                    text-transform: capitalize


                @media(max-width: 991px)
                    input 
                    label 
                        display: initial

            .rating-avalie-wrapper
                margin: 0 0 0 8px

            h5 
                font-size: 13px
                color: #333333
                letter-spacing: -0.24px
                font-weight: 600
                text-transform: capitalize


        #ulPubliqueResenha
            grid-area: ulPubliqueResenha
            // align-self: start
            a  
                background: #333333
                border-radius: 2px
                font-size: 14px
                color: #DEB94C
                letter-spacing: -0.25px
                width: 170px
                height: 30px
                display: flex
                align-items: center
                justify-content: center
                text-transform: uppercase
                font-weight: 600
                margin: 10px 0 0 0


        .links-resenha-usuario
            grid-area: links-resenha-usuario
            display: none


        #userReviewReturnMessages
            grid-area: userReviewReturnMessages

        
        .resenhas
            grid-area: resenhas
            margin: 20px 0

            .opt-texto
                color: #616161
                font-size: 13px
                text-transform: initial
                // margin: 10px 0
                padding: 10px 0
                // background: rgba(0,0,0,.05)

            .util
                display: none

            .quem
                text-transform:uppercase
                color: #726F6F
                font-size: 12px
                
                > li + li
                    margin-top: 20px
                li 
                    margin-top: 40px
                    margin-bottom:40px
                    .quem 
                        display: flex

                    &:last-child 
                        padding-bottom: 40px 
                        border-bottom: 1px solid #E9E9E9


                .dados 
                    strong 
                        font-weight: 400


    .rating-avalie
        a  
            text-decoration: none
            &:visited
            &:focus
            &:hover 
                text-decoration: none

        #rtAvaliacao_A 
            display: flex
            align-items: center
            font-size: 0
            line-height: 0

            span 
                display: block
                height: 20px
                width: 20px
                overflow: hidden
                &:before 
                    @extend $star 
                    background-position: -100px 0

                &.filledRatingStar
                    &:before 
                        background-position: -5px 0
                    &:hover 
                        &:before 
                            background-position: -5px 0
                    


